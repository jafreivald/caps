<%= form_tag sessions_path do %>
  <div class="form-group">
      <%= label_tag :userid %><br />
      <%= text_field_tag :userid, params[:user_id], :class => "form-control" %>
  </div>
  <div class="form-group">
      <%= label_tag :password %><br />
      <%= password_field_tag :password, :class => "form-control" %>
  </div>
  <div class="actions">
      <%= submit_tag "Log In", :class => "submit_record btn btn-primary"  %>
  </div>
<% end %>

