<h1>Listing phone_types</h1>

<table class="display">
    <thead>
        <tr>
            <th>Phone type</th>
            <th>Modify</th>
        </tr>
    </thead>
    <tbody>
        <% @phone_types.each do |phone_type| %>
          <tr>
              <td><%= phone_type.phone_type %></td>
              <td><%= link_to 'Show', phone_type %>
                  <%= link_to 'Edit', edit_phone_type_path(phone_type) %>
                  <%= link_to 'Destroy', phone_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
    </tbody>
</table>

<br />

<%= link_to 'New Phone type', new_phone_type_path %>
